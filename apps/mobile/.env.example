# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Web API URL (for API calls to the web app)
EXPO_PUBLIC_WEB_URL=https://www.myteamnetwork.com

# Google OAuth (optional - required for Google Sign-In on native)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-google-web-client-id
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-google-ios-client-id

# Analytics - PostHog (product analytics)
EXPO_PUBLIC_POSTHOG_KEY=phc_xxxxx

# Error Tracking - Sentry
EXPO_PUBLIC_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx

# hCaptcha (donations/Stripe)
EXPO_PUBLIC_HCAPTCHA_SITE_KEY=your-hcaptcha-site-key
EXPO_PUBLIC_HCAPTCHA_BASE_URL=https://www.myteamnetwork.com
